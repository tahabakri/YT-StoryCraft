# YouTube API Key for video search and metadata
NEXT_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here

# YouTube OAuth Credentials (Required for caption management)
# Get these from Google Cloud Console: https://console.cloud.google.com/apis/credentials
YOUTUBE_CLIENT_ID=your_client_id_here
YOUTUBE_CLIENT_SECRET=your_client_secret_here

# Elevenlabs API Key for text-to-speech
NEXT_PUBLIC_VITE_ELEVEN_LABS_API_KEY=your_elevenlabs_api_key_here

# Groq API Key for AI text generation
NEXT_PUBLIC_GROQ_API_KEY=your_groq_api_key_here
